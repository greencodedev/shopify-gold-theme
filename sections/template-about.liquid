                {% else %}
                <img class="img-responsive" alt="{{ shop.name }}" src="//placehold.it/530X340" />
                {% endif %}
                {% if section.settings.image2_link != blank %}
              </a>
              {% endif %}
              <h3>{{ section.settings.title2 }}</h3>
              <div class="des">{{ section.settings.description2 }}</div>
            </div>
          </div>
        </div>
        <div class="item col-md-4 col-xs-12">
          <div class="item_wrap">
            <div class="image service">
              {% if section.settings.image3_link != blank %}
              <a href="{{ section.settings.about_link }}" title="{{ section.settings.about_link.alt | default: shop.name }}">
                {% endif %}
                {% if section.settings.image3 != blank %}
                <img class="img-responsive lazyload" data-sizes="auto" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" alt="{{ section.settings.image3.alt | default: shop.name }}" data-src="{{ section.settings.image3 | img_url: 'master' }}" />
                {% else %}
                <img class="img-responsive" alt="{{ shop.name }}" src="//placehold.it/530X340" />
                {% endif %}
                {% if section.settings.image3_link != blank %}
              </a>
              {% endif %}
              <h3>{{ section.settings.title3 }}</h3>
              <div class="des">{{ section.settings.description3 }} </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
{% endif %}
</div>
{% endif %}

{% if section.settings.show_team != blank %}
<div class="our-member">
  <div class="container">
    <div class="wrap">
      {% if section.settings.member_title != blank %}
      <div class="title-about-us">
        <h2>{{ section.settings.member_title }}</h2>
      </div>
      {% endif %}

      {% if section.settings.our_text_desc != blank %}
      <div class="dshort-des">
        {{ section.settings.our_text_desc }}
      </div>
      {% endif %}
      <div class="row">
        {% for block in section.blocks %}
        {% case block.type %}
        {% when 'faq' %}   
        <div class="col-xl-3 col-lg-3 col-6">
          <div class="item-inner">
            <div class="w-image-box">
              {% if block.settings.our_image_1 != blank %}
              <div class="item-image">
                <a title="Jennifer lawrence" href="#">
                  <img class="img-respon" alt="{{ block.settings.our_image_1.alt | default: shop.name }}" src="{{ block.settings.our_image_1 | img_url: 'master' }}" />
                </a>
              </div>
              {% else %}
              <img class="img-responsive" alt="{{ shop.name }}" src="//placehold.it/160x160" />
              {% endif %}
            </div>
            <div class="info-member">
              <div class="item-content">
              {% if block.settings.cl_name_1 != blank %}
              <h2 class="cl-name"><a title="Jennifer lawrence" href="#">{{ block.settings.cl_name_1 }}</a></h2>
              {% endif %}
              {% if block.settings.cl_job_1 != blank %}
              <p class="cl-job">{{ section.settings.cl_job_1 }}Tech Leader</p>
              {% endif %}
              </div>
              <div class="socials-wraps "> 
                <ul>

                  {% if block.settings.social_facebook_link %}
                  <li class="facebook"><a class="_blank" href="{{ block.settings.social_facebook_link }}" target="_blank"><i class="fa fa-facebook"></i></a></li>
                  {% endif %}
                  {% if block.settings.social_twitter_link %}
                  <li class="twitter"><a class="_blank" href="{{ block.settings.social_twitter_link }}" target="_blank"><i class="fa fa-twitter"></i></a></li>
                  {% endif %}
                  {% if block.settings.social_google_link %}
                  <li class="google_plus"><a class="_blank" href="{{ block.settings.social_google_link }}" target="_blank"><i class="fa  fa-google-plus"></i></a></li>
                  {% endif %}
                  {% if block.settings.social_dribbble_link %}
                  <li class="dribbble"><a class="_blank" href="{{ block.settings.social_dribbble_link }}" target="_blank"><i class="fa  fa-dribbble"></i></a></li>
                  {% endif %}            
                  {% if block.settings.social_instagram_link %}
                  <li class="instagram"><a class="_blank" href="{{ block.settings.social_instagram_link }}" target="_blank"><i class="fa  fa-instagram"></i></a></li>
                  {% endif %}
                  {% if block.settings.social_pinterest_link %}
                  <li class="pinterest"><a class="_blank" href="{{ block.settings.social_pinterest_link }}" target="_blank"><i class="fa fa-pinterest"></i></a></li>
                  {% endif %}
                  {% if block.settings.social_linkedin_link %}
                  <li class="linkedin"><a class="_blank" href="{{ block.settings.social_linkedin_link }}" target="_blank"><i class="fa fa-linkedin"></i></a></li>
                  {% endif %}            
                </ul>
              </div>  
            </div>
          </div>
        </div>
        {% endcase %}
        {% endfor %}        
      </div>
    </div>	
  </div>
</div>
{% endif %}
<style>
  .main-content{
  	padding-bottom: 0 !important; 
  }
</style> 
