{% if section.settings.style == '1' %}  
<div class="about-us style1">
<div class="container page-about">
  <div class="row">
    <div class="col-md-12 welcome-about-us">
      <div class="content-about-us row">
        {% if section.settings.about_image != blank %}
        <div class="image-about-us col-lg-6 col-md-12 col-12">
          <img class="img-respon" alt="{{ section.settings.about_image.alt | default: shop.name }}" src="{{ section.settings.about_image | img_url: 'master' }}" />
        </div>
        {% else %}
        <div class="image-about-us col-lg-6 col-md-12 col-12">
          <img class="img-responsive" alt="{{ shop.name }}" src="//placehold.it/570x402" />
        </div>
        {% endif %}
        <div class="content-about col-lg-6 col-md-12 col-12">

          {% if section.settings.about_title != blank %}
          <div class="title-about-us">
            <h2>{{ section.settings.about_title }}</h2>
          </div>
          {% endif %}
          {% if section.settings.about_text_desc != blank %}
          <div class="des-about-us">
            {{ section.settings.about_text_desc }}
          </div>
          {% endif %}
          {% if section.settings.about_link != blank %}
          <a class="btn-contact" href="{{ section.settings.about_link }}">
            contact us
          </a>
          {% endif %}          
        </div>
      </div>
    </div>
  </div>
  <div class="wpb_wrapper">
  </div>  
</div>
{% if section.settings.show_service != blank %}
<div class="about-service">
  <div class="container">
    <div class="wrap">
      <div class="row">
        <div class="item col-md-4 col-12">
          <div class="item_wrap">
            <div class="icon"><i class="fa fa-bar-chart"></i></div>
            <h3>{{ section.settings.icon1 }}</h3>
            <div class="des">{{ section.settings.des1 }}</div>
          </div>
        </div>
        <div class="item col-md-4 col-12">
          <div class="item_wrap">
            <div class="icon"><i class="fa fa-paint-brush"></i></div>
            <h3>{{ section.settings.icon2 }}</h3>
            <div class="des">{{ section.settings.des2 }}</div>
          </div>
        </div>
        <div class="item col-md-4 col-12">
          <div class="item_wrap">
            <div class="icon"><i class="fa fa-code"></i></div>
            <h3>{{ section.settings.icon3 }}</h3>
            <div class="des">{{ section.settings.des3 }} </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
{% endif %}
</div>
{% else %}	

<div class="about-us style2">
<div class="container page-about">
  <div class="row">
    <div class="col-md-12 welcome-about-us banners">
      <div class="content-about-us">
        {% if section.settings.about_link != blank %}
        <a href="{{ section.settings.about_link }}" title="{{ section.settings.about_link.alt | default: shop.name }}">
          {% endif %}
          {% if section.settings.about_image2 != blank %}
          <div class="image-about-us col-md-12 col-sm-12">
            <img class="img-respon" alt="{{ section.settings.about_image2.alt | default: shop.name }}" src="{{ section.settings.about_image2 | img_url: 'master' }}" />
          </div>
          {% else %}
          <div class="image-about-us col-md-12 col-sm-12">
            <img class="img-responsive" alt="{{ shop.name }}" src="//placehold.it/1650x500" />
          </div>
          {% endif %}
          {% if section.settings.about_link != blank %}
        </a>
        {% endif %}
        <div class="content-about col-md-12 col-sm-12">
          {% if section.settings.about_title != blank %}
          <div class="title-about-us">
            <h2>{{ section.settings.about_title }}</h2>
          </div>
          {% endif %}
          {% if section.settings.about_text_desc != blank %}
          <div class="des-about-us">
            {{ section.settings.about_text_desc }}
          </div>
          {% endif %}          
        </div>
      </div>
    </div>
  </div>
  <div class="wpb_wrapper">
  </div>  
</div>
{% if section.settings.show_service != blank %}
<div class="about-service">
  <div class="container">
