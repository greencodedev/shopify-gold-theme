                {% else %}
                <img class="img-responsive" alt="{{ shop.name }}" src="//placehold.it/530X340" />
                {% endif %}              
                {% if section.settings.image2_link != blank %}
              </a>
              {% endif %} 
              <h3>{{ section.settings.title1 }}</h3>
              <div class="des">{{ section.settings.description1 }}</div>
            </div>
          </div>
        </div>
        <div class="item col-md-4 col-xs-12">
          <div class="item_wrap">
            <div class="image service">
              {% if section.settings.image2_link != blank %}
